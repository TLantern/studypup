{
  "expo": {
    "name": "StudyPup",
    "slug": "studypup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/mainicon.png",
    "scheme": "studypup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "StudyPup needs local network access to connect to the development server for live reload.",
        "NSMicrophoneUsageDescription": "StudyPup uses the microphone to record lectures and study notes.",
        "NSCameraUsageDescription": "StudyPup uses the camera to capture notes and study materials.",
        "NSPhotoLibraryUsageDescription": "StudyPup needs access to your photos to import study materials and images.",
        "NSPhotoLibraryAddUsageDescription": "StudyPup saves recordings and captures to your photo library."
      },
      "icon": {
        "light": "./assets/icons/appcovericon.png",
        "dark": "./assets/icons/appcovericon.png",
        "tinted": "./assets/icons/appcovericon.png"
      },
      "bundleIdentifier": "com.anonymous.studypup"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#add8e6",
        "foregroundImage": "./assets/icons/appcovericon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.studypup",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "StudyPup needs access to your photos to import study materials and images.",
          "cameraPermission": "StudyPup uses the camera to capture notes and study materials.",
          "microphonePermission": "StudyPup uses the microphone to record lectures and study notes."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "StudyPup needs access to your photos to import study materials and images.",
          "savePhotosPermission": "StudyPup saves recordings and captures to your photo library."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#7AA3A3",
          "image": "./assets/icons/mainicon.png",
          "dark": {
            "image": "./assets/icons/mainicon.png",
            "backgroundColor": "#7AA3A3"
          },
          "imageWidth": 200
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": { "minSdkVersion": 21 },
          "ios": {
            "deploymentTarget": "15.1",
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73cf338b-2d1f-4987-926b-935e6f5863a4"
      }
    },
    "owner": "ten123"
  }
}
